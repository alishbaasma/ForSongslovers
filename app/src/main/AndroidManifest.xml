<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sab.soundcloud">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".PermissionScreen" />
        <activity android:name=".AccountSignIn" />
        <activity android:name=".SignIn" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".googleSignIn" />
        <activity android:name=".fblogin" />
        <activity android:name=".Home" />
        <activity android:name=".songPlayer"></activity>
    </application>

</manifest>